<Window x:Class="DPAPI___Wpf.Chrome_History_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DPAPI___Wpf"
        mc:Ignorable="d"
        Title="Chrome_History_Window" Height="{Binding SystemParameters.PrimaryScreenHeight}" Width="{Binding SystemParameters.PrimaryScreenWidth}">

    <Grid>
        <ScrollViewer SizeChanged="ScrollViewer_SizeChanged">
            <DataGrid x:Name="gv" IsReadOnly="True" MouseDoubleClick="gv_MouseDoubleClick" HorizontalAlignment="Left" Height="392" VerticalAlignment="Top" Width="753" SelectionChanged="gv_SelectionChanged"/>
        </ScrollViewer>
    </Grid>
</Window>
